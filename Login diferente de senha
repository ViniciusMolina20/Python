
nome_usuario = input("Informe nome de usuario: ")
senha_usuario = input("Informe a senha: ")

while nome_usuario == senha_usuario:
    print ("\nNome de Usuario e senha n√£o podem ser iguais")
    nome_usuario = input("Informe nome de usuario: ")
    senha_usuario = input("Informe a senha: ")


print ("\nBem vindo!!!")
